<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="uv">

	<select id="select_list" resultType="vo.DirectRadiationVO">
		select * from direct_radiation
	</select>
	
	<select id="select_city_with_idx" resultType="vo.CityVO" parameterType="int">
		select * from city where idx = #{city_idx}
	</select>
	
	<select id="select_city_list" resultType="vo.CityVO">
		select * from city
	</select>
	
	<select id="select_direct_radiation_list" resultType="vo.DirectRadiationVO" parameterType="java.util.Map">
		select * from direct_radiation where latitude = #{latitude} and longitude = #{longitude}
	</select>
 	
</mapper>












